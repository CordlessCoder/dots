# IMPORTANT: colors have to be defined in quotation marks.
# Otherwise the "#" and following would be interpreted as a comment.
[urgency_low]
    frame_color = "#EB6EB7"
    foreground = "#b9b9b9"
    background = "#181818"

[urgency_normal]
    frame_color = "#368AEB"
    foreground = "#b9b9b9"
    background = "#181818"

[urgency_critical]
    frame_color = "#70B433"
    foreground = "#b9b9b9"
    background = "#181818"
