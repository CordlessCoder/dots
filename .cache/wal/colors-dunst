# IMPORTANT: colors have to be defined in quotation marks.
# Otherwise the "#" and following would be interpreted as a comment.
[urgency_low]
    frame_color = "#bb9af7"
    foreground = "#c0caf5"
    background = "#11121D"

[urgency_normal]
    frame_color = "#7aa2f7"
    foreground = "#c0caf5"
    background = "#11121D"

[urgency_critical]
    frame_color = "#9ece6a"
    foreground = "#c0caf5"
    background = "#11121D"
